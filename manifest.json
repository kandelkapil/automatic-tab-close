{
  "manifest_version": 2,
  "name": "Automatic Tab Closer",
  "version": "1.0",
  "description": "Close specific domain tabs automatically.",
  "permissions": ["tabs", "storage","webNavigation"],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
